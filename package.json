{
  "name": "wueste",
  "version": "0.0.0",
  "description": "A Serialization and Generator Framework",
  "scripts": {
    "clean": "npx rimraf dist src/generated node_modules",
    "build": "npm run build:wasm; npm run build:js",
    "build:js": "tsc",
    "build:wasm": "mkdir -p dist; cp \"$(go env GOROOT)/misc/wasm/wasm_exec.js\"  \"$(go env GOROOT)/misc/wasm/wasm_exec_node.js\"  dist/; GOOS=js GOARCH=wasm go build -o dist/generator.wasm entity-generator/cmd/generator.go",
    "test": "npm run test:go && npm run test:js",
    "test:js": "jest",
    "test:go": "go test ./...",
    "prepublish": "npm run build; rm -rf pubdir ; mkdir -p pubdir; cp -pr .gitignore .npmignore package.json README.md bin/generator dist/ pubdir/",
    "publish": "cd pubdir; npm ",
    "pregenerator": "npm run build:wasm",
    "generator": "bin/generator --write-test-schema  --input-file ./src/generated/wasm/simple_type.schema.json --eg-from-wueste ../..wueste --eg-from-result ../../result --output-dir ./src/generated/wasm",
    "postgenerator": "npm run build:js",
    "presmoke": "npm run publish pack",
    "smoke": "cd smoke; npm init -y -f ; npm install -f ../pubdir/wueste-*.tgz; npx ts-node smoke.ts; npx wueste wueste-generator --write-test-schema",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mabels/wueste.git"
  },
  "keywords": [
    "serialization",
    "generator",
    "typescript"
  ],
  "author": "Meno Abels <meno.abels@adviser.com>",
  "license": "AFL-2.0",
  "bugs": {
    "url": "https://github.com/mabels/wueste/issues"
  },
  "homepage": "https://github.com/mabels/wueste#readme",
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "esbuild": "^0.18.14",
    "eslint": "^8.45.0",
    "husky": "^8.0.0",
    "jest": "^29.6.1",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=16"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "bin": {
    "wueste-generator": "generator"
  }
}
