{
  "name": "wueste",
  "version": "0.0.0",
  "description": "A Serialization and Generator Framework",
  "scripts": {
    "clean": "npx rimraf dist src/generated node_modules",
    "prebuild": "pnpm run generator",
    "build": "pnpm run build:wasm; pnpm run build:js",
    "build:js": "tsc",
    "build:wasm": "mkdir -p dist; cp \"$(go env GOROOT)/misc/wasm/wasm_exec.js\"  \"$(go env GOROOT)/misc/wasm/wasm_exec_node.js\"  dist/; GOOS=js GOARCH=wasm go build -o dist/generator.wasm entity-generator/cmd/generator.go",
    "pretest": "rm -rf pubdir",
    "test": "pnpm run test:go && pnpm run test:js",
    "pretest:go": "pnpm run generator",
    "test:js": "jest",
    "test:go": "go test ./...",
    "pubdir": "pnpm run build; rm -rf pubdir ; mkdir -p pubdir; cp -pr .gitignore .npmignore package.json README.md bin/generator pubdir/; cd dist ; cp -pr . ../pubdir/",
    "pregenerator": "rm -rf src/generated/wasm; pnpm run build:wasm",
    "generator": "bin/generator --write-test-schema  --input-file ./src/generated/wasm/simple_type.schema.json --input-file ./src/generated/wasm/base.schema.json --input-file ./src/helper.test.schema.json --eg-from-wueste ../../wueste --eg-from-result ../../result --output-dir ./src/generated/wasm && bin/generator --eg-from-wueste ../wueste --eg-from-result ../result --input-file ./src/payload.schema.json --output-dir ./src/generated/",
    "postgenerator": "pnpm run build:js",
    "presmoke": "pnpm run pubdir ; cd pubdir ; pnpm pack",
    "smoke": "cd smoke; rm -f package.json; pnpm init ; pnpm install -f ../pubdir/wueste-*.tgz; npx wueste wueste-generator --write-test-schema --input-file ./generated/simple_type.schema.json --output-dir ./generated; npx ts-node ./smoke.ts",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mabels/wueste.git"
  },
  "keywords": [
    "serialization",
    "generator",
    "typescript"
  ],
  "author": "Meno Abels <meno.abels@adviser.com>",
  "license": "AFL-2.0",
  "bugs": {
    "url": "https://github.com/mabels/wueste/issues"
  },
  "homepage": "https://github.com/mabels/wueste#readme",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.2",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "esbuild": "^0.18.20",
    "eslint": "^8.50.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "bin": {
    "wueste-generator": "generator"
  },
  "dependencies": {
    "@cmdcode/buff": "^2.0.3",
    "@noble/hashes": "^1.3.2",
    "base58-js": "^2.0.0"
  }
}
